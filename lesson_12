a = int(input())
b = int(input())
c = int(input())

if b <= a >= c:
    print(a)
    if b >= c:
        print(c)
        print(b)
    else:
        print(b)
        print(c)
elif a <= b >= c:
    print(b)
    if a >= c:
        print(c)
        print(a)
    else:
        print(a)
        print(c)
elif a <= c >= b:
    print(c)
    if a <= b:
        print(a)
        print(b)
    else:
        print(b)
        print(a)
