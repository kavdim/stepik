a = int(input())

if 0 <= a <= 1000:
    if (a % 10) == 0:
        print(a, "программистов")
    elif (a % 10) == 1:
        if 10 < (a % 100) < 20:
            print(a, "программистов")
        else:
            print(a, "программист")
    elif 2 <= (a % 10) <= 4:
        if 10 < (a % 100) < 20:
            print(a, "программистов")
        else:
            print(a, "программиста")
    elif 5 <= (a % 10) <= 9:
        print(a, "программистов")
else:
    print("Неверное число.")
